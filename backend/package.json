{
  "name": "ovce-databaze-api",
  "version": "1.0.0",
  "description": "REST API for sheep farm management system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "import-data": "node scripts/import-original-data.js",
    "import-data-clear": "node scripts/import-original-data.js --clear-existing",
    "migrate-and-import": "node scripts/migrate-and-import.js",
    "build": "echo 'Build completed'",
    "railway-deploy": "npm install && npm run migrate-and-import"
  },
  "keywords": [
    "api",
    "sheep",
    "farming",
    "railway"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "railway": {
    "build": {
      "command": "npm install"
    },
    "start": {
      "command": "npm start"
    }
  }
}
